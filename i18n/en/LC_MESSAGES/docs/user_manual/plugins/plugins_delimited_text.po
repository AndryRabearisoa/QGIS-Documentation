#: ../../source/user_manual/plugins/plugins_delimited_text.rst:40
# 0730248f09904f51a31fa8c249c2ff29
msgid "Some items of note about the text file are:"
msgstr ""

#: ../../source/user_manual/plugins/plugins_delimited_text.rst:43
# 1d7476ea872444a1aa80dbb194be2e70
