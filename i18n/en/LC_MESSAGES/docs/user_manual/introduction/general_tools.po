#: ../../source/user_manual/introduction/general_tools.rst:301
# 7947a4a184ff4c2ea8358c26fa5298ac
msgid "Copyright Label"
msgstr ""

#: ../../source/user_manual/introduction/general_tools.rst:304
# 8809aa8e35844a00a4afc1299c3c7f76
msgid "|copyright_label| :menuselection:`Copyright label` adds a Copyright label using the text you prefer to the map."
msgstr ""

#: ../../source/user_manual/introduction/general_tools.rst:320
# 838ff38807a24917b9730a2001ee58e5
msgid "Select from menu :menuselection:`View --> Decorations --> Copyright Label`. The dialog starts (see figure_decorations_1_)."
msgstr ""

#: ../../source/user_manual/introduction/general_tools.rst:322
# 0e3f63fa5c1145c5bb14cda1e9086419
msgid "Enter the text you want to place on the map. You can use HTML as shown in the example"
