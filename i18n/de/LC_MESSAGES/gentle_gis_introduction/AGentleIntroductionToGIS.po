#: ../../source/gentle_gis_introduction/AGentleIntroductionToGIS.rst:687
msgid "type of dwelling (RDP House, block of flats, shack, brick house etc)"
msgstr ""

# e2bbdc59920649d59704ecfcde8906d3
#: ../../source/gentle_gis_introduction/AGentleIntroductionToGIS.rst:689
msgid "year the house was built"
